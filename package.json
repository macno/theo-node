{
  "name": "theo",
  "version": "0.18.4",
  "description": "Theo is your keys manager",
  "main": "index.js",
  "repository": "theoapp/theo-node",
  "scripts": {
    "build": "babel -D src -d build",
    "build:dev": "babel -D --watch src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "run:dev": "nodemon  --watch build ./build/index.js",
    "start:dev": "concurrently --kill-others \"npm run build:dev\" \"sleep 5 && npm run run:dev\"",
    "start": "node ./build/index.js",
    "test": "mocha build/test",
    "test:standalone": "mocha build/test/testStandAlone*",
    "test:api": "mocha build/test/testRest*",
    "test:api:signed": "mocha build/test/testSigned*",
    "test:core": "mocha build/test/testCore.js",
    "test:core:restart": "mocha build/test/testCoreOnRestart.js",
    "test:core:noassignee": "mocha build/test/testCoreNoAssignee.js",
    "test:core:restart:noassignee": "mocha build/test/testCoreOnRestartNoAssignee.js",
    "test:cluster": "mocha build/test/testCluster.js",
    "test:sshutils": "mocha build/test/testSSHUtils.js",
    "version": "./updateVersion.sh"
  },
  "author": "Michele <michele@azzolari.it> (macno)",
  "license": "Apache-2.0",
  "dependencies": {
    "@authkeys-io/microservice": "^2.1.1",
    "dotenv": "^6.2.0",
    "memcached": "^2.2.2",
    "mysql2": "^1.7.0",
    "node-fetch": "^2.6.0",
    "redis": "^2.8.0",
    "sqlite3": "^4.1.0",
    "sshpk": "^1.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "event-stream": "3.3.4",
    "mocha": "^6.0.2"
  },
  "optionalDependencies": {
    "babel-eslint": "^10.0.2",
    "concurrently": "^3.5.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.5.3"
  }
}
